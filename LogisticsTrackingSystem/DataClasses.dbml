      <Association Name="parcel_parcel_log" Member="parcel" ThisKey="parcel_id" OtherKey="tracking_no" Type="parcel" IsForeignKey="true" />
    </Type>
  </Table>
  <Table Name="dbo.parcel_routes" Member="parcel_routes">
