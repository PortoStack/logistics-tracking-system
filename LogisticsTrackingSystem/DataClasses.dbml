      <Column Name="contact" Type="System.String" DbType="VarChar(255)" CanBeNull="true" />
      <Column Name="location" Type="System.String" DbType="VarChar(MAX)" CanBeNull="true" />
      <Association Name="warehouse_parcel" Member="parcels" ThisKey="id" OtherKey="destination_id" Type="parcel" />
      <Association Name="warehouse_parcel1" Member="parcels1" ThisKey="id" OtherKey="origin_id" Type="parcel" />
      <Association Name="warehouse_route" Member="routes" ThisKey="id" OtherKey="destination_id" Type="route" />
      <Association Name="warehouse_route1" Member="routes1" ThisKey="id" OtherKey="origin_id" Type="route" />
    </Type>
  </Table>
  <Table Name="dbo.drivers" Member="drivers">
    <Type Name="driver">
      <Column Name="id" Type="System.Int32" DbType="Int NOT NULL IDENTITY" IsPrimaryKey="true" IsDbGenerated="true" CanBeNull="false" />
