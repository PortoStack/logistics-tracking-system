<?xml version="1.0" encoding="utf-8"?><Database Name="LogisticsDB" Class="DataClassesDataContext" xmlns="http://schemas.microsoft.com/linqtosql/dbml/2007">
  <Table Name="dbo.customers" Member="customers">
    <Type Name="customer">
      <Column Name="id" Type="System.Int32" DbType="Int NOT NULL IDENTITY" IsPrimaryKey="true" IsDbGenerated="true" CanBeNull="false" />
      <Column Name="name" Type="System.String" DbType="VarChar(100) NOT NULL" CanBeNull="false" />
      <Column Name="email" Type="System.String" DbType="VarChar(100) NOT NULL" CanBeNull="false" />
      <Column Name="phone" Type="System.String" DbType="VarChar(20) NOT NULL" CanBeNull="false" />
      <Column Name="type" Type="System.String" DbType="VarChar(20) NOT NULL" CanBeNull="false" />
      <Association Name="customer_parcel" Member="parcels" ThisKey="id" OtherKey="receiver_id" Type="parcel" />
      <Association Name="customer_parcel1" Member="parcels1" ThisKey="id" OtherKey="sender_id" Type="parcel" />
    </Type>
  </Table>
  <Table Name="dbo.warehouses" Member="warehouses">
    <Type Name="warehouse">
